	<queryString language="SQL">
		<![CDATA[SELECT
    d.name AS Nome,
    CASE WHEN s.id IS NOT NULL THEN 'Vendido' ELSE 'Disponível' END AS Status,
    c.name AS Cliente,
    c.type AS Tipo_Cliente,
    s.price AS Valor
FROM
    ducks d
LEFT JOIN
    sales s ON d.id = s.id_duck
LEFT JOIN
    clients c ON s.id_client = c.id
ORDER BY
    d.id;]]>
